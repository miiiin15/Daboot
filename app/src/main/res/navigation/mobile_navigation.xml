<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.daboot.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_board"
        android:name="com.example.daboot.ui.dashboard.DashboardFragment"
        android:label="@string/title_board"
        tools:layout="@layout/fragment_board" />

    <fragment
        android:id="@+id/navigation_matching"
        android:name="com.example.daboot.ui.notifications.NotificationsFragment"
        android:label="@string/title_matching"
        tools:layout="@layout/fragment_matching" />
    <fragment
        android:id="@+id/navigation_welfare"
        android:name="com.example.daboot.navigation_welfare"
        android:label="@string/title_welfare"
        tools:layout="@layout/fragment_welfare" />
    <fragment
        android:id="@+id/navgation_info"
        android:name="com.example.daboot.Info"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_info" />
</navigation>